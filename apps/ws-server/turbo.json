{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "$TURBO_DEFAULT$"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "dev": {
            "cache": false
        },
        "start": {
            "dependsOn": [
                "build"
            ],
            "cache": false
        }
    }
}